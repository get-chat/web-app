{
	"name": "getchat-frontend",
	"version": "local",
	"private": true,
	"scripts": {
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"start": "webpack serve --config webpack/webpack.dev.js",
		"build": "webpack --config webpack/webpack.prod.js",
		"preview": "serve -s build",
		"test": "jest",
		"test:e2e": "cypress run",
		"i18next-conv": "lang=en;i18next-conv --compatibilityJSON v4 -l $lang -s ./src/locales/${lang}/translation.po -t ./src/locales/${lang}/translation.json",
		"prepare": "husky install",
		"lint:staged": "lint-staged --allow-empty",
		"lint:prettier": "prettier --check --ignore-unknown  src",
		"lint:prettier:fix": "prettier --write --ignore-unknown src"
	},
	"lint-staged": {
		"src/**/*": "pnpm lint:prettier"
	},
